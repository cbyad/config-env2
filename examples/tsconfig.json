{
  "compilerOptions": {
    "outDir": "dist",
    "typeRoots": [
      "node_modules/@types",
      "./typings"
    ],
  },
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "DOM",
      "ESNext",
      "ES2015",
      "ES6",
      "ES2017"
    ],
    "noUnusedParameters": true,
    "importHelpers": true,
    "declaration": true,
    "rootDir": "src",
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowJs": false,
    "outDir": "dist",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "forceConsistentCasingInFileNames": true,
    "exclude": [
      "node_modules",
      "dist"
    ],
    "include": [
      "src",
      "index.ts"
    ]
  }